{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "akaunting/laravel-money": "^6.0",
        "barryvdh/laravel-dompdf": "^3.1",
        "binance/binance-connector-php": "^2.0",
        "doctrine/dbal": "^4.3",
        "florianv/exchanger": "^2.8",
        "guzzlehttp/guzzle": "^7.10",
        "intervention/image": "*",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.19",
        "laravel/socialite": "^5.23",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.29",
        "league/html-to-markdown": "^5.1",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^2.3",
        "moneyphp/money": "^4.7",
        "mpdf/mpdf": "^8.2",
        "owen-it/laravel-auditing": "^14.0",
        "php-http/guzzle7-adapter": "^1.1",
        "pragmarx/google2fa-laravel": "^2.3",
        "predis/predis": "^3.2",
        "ramsey/uuid": "^4.9",
        "spatie/image-optimizer": "^1.8",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-backup": "^9.3",
        "spatie/laravel-database-mail-templates": "^3.7",
        "spatie/laravel-fractal": "^6.3",
        "spatie/laravel-html": "^3.12",
        "spatie/laravel-image-optimizer": "^1.8",
        "spatie/laravel-markdown": "^2.7",
        "spatie/laravel-medialibrary": "^11.14",
        "spatie/laravel-permission": "^6.21",
        "spatie/laravel-query-builder": "^6.3",
        "spatie/laravel-rate-limited-job-middleware": "^2.8",
        "spatie/laravel-schedule-monitor": "^4.0",
        "spatie/laravel-searchable": "^1.13",
        "spatie/laravel-server-monitor": "^1.10",
        "spatie/laravel-sitemap": "^7.3",
        "spatie/laravel-translatable": "^6.11",
        "spatie/laravel-validation-rules": "^3.4",
        "stripe/stripe-php": "^17.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "fakerphp/faker": "^1.24",
        "larastan/larastan": "^3.6",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "laravel/telescope": "^5.11",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5",
        "rector/rector": "^2.1",
        "spatie/laravel-ray": "^1.40"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
